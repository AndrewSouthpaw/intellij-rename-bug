  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-flow": "^6.23.0",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.6.0",
    "eslint": "^4.7.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.54.1",
    "pre-commit": "^1.2.2",
    "q": "^1.5.0",
    "react-addons-perf": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-test-renderer": "15.4.2",
    "redux-devtools": "^3.3.1",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "^2.4.5"
  },
  "engines": {
    "node": "6.3.x",
    "npm": "5.5.x"
  },
  "scripts": {
    "postinstall": "./node_modules/bower/bin/bower install",
    "webpack:deploy": "./node_modules/.bin/webpack --config=webpack.config.js -p",
    "heroku-postbuild": "npm run webpack:deploy",
    "flow": "flow",
    "lint": "flow",
    "test": "jest -e",
    "test:watch": "jest --watch -e --ci"
  },
  "pre-commit": [
    "lint"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "frontend"
    ],
    "moduleNameMapper": {
      "^config$": "<rootDir>/configs/app-config.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(scss|css|less)$": "identity-obj-proxy",
      "^modules/(.*)": "<rootDir>/frontend/src/modules/$1",
      "^test_helpers/(.*)": "<rootDir>/frontend/src/test_helpers/$1",
      "^shared/(.*)": "<rootDir>/frontend/src/shared/$1"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/setup-test-framework-script.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "**/frontend/**/__tests__/*.test.js?(x)",
      "**/frontend/**/__tests__/*_spec.js?(x)"
    ]
  }
}
